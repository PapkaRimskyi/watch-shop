{
  "name": "watch-shop",
  "version": "1.0.0",
  "description": "watch-shop",
  "scripts": {
    "editorconfig": "editorconfig",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "eslint": "eslint source/**/*.{js,jsx}",
    "check": "npm run editorconfig && npm run stylelint && npm run eslint",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development",
    "svgo": "svgo -r -f source/img/"
  },
  "browserslist": [
    "> 1%",
    "last 3 version",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PapkaRimskyi/watch-shop.git"
  },
  "author": "PR",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PapkaRimskyi/watch-shop/issues"
  },
  "homepage": "https://github.com/PapkaRimskyi/watch-shop#readme",
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.1",
    "css-loader": "3.5.3",
    "editorconfig": "0.15.3",
    "eslint": "7.0.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "image-webpack-loader": "6.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "stylelint": "13.5.0",
    "stylelint-config-recommended": "3.0.0",
    "svgo": "1.3.2",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "autoprefixer": "9.8.0",
    "classnames": "2.2.6",
    "core-js": "3.6.5",
    "cssnano": "4.1.10",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-transition-group": "4.4.1"
  },
  "editorconfig": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.{js,jsx}",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ]
}
